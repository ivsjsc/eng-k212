{
  "compilerOptions": {
    "target": "ES2022",
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "module": "ESNext",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "skipLibCheck": true,
    "types": [
      "node",
      "vite/client"
    ],
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "moduleDetection": "force",
    "allowJs": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "allowImportingTsExtensions": true,
    "noEmit": true
  }
  ,
  "exclude": [
    "archive/**",
    "node_modules",
    "netlify/**",
    "scripts/**",
    "services/**",
    "src/main-auth0-wrapper.tsx",
    "src/Auth0route.tsx",
    "App_old.tsx"
  ],
  "include": [
    "src/**/*",
    "App.tsx",
    "components/**/*",
    "data/**/*"
  ]
}